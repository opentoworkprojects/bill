{
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/.well-known/assetlinks.json",
      "destination": "https://restro-ai.onrender.com/.well-known/assetlinks.json"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
